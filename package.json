{
	"name": "obsidian-svelte-syntax-highlighter",
	"version": "1.0.3",
	"description": "This plugin adds Svelte syntax highlighter to Obsidian. (https://obsidian.md)",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [
		"obsidian",
		"plugin",
		"svelte"
	],
	"author": {
		"name": "typhoon-kim",
		"email": "typh007.kim@gmail.com",
		"url": "https://github.com/typhoon-kim"
	},
	"homepage": "https://typhoon-kim.github.io/obsidian-svelte-syntax-highlighter/",
	"repository": {
		"type": "git",
		"url": "https://github.com/typhoon-kim/obsidian-svelte-syntax-highlighter"
	},
	"bugs": {
		"url": "https://github.com/typhoon-kim/obsidian-svelte-syntax-highlighter/issues",
		"email": "typh007.kim@gmail.com"
	},
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"@codemirror/state": "^6.4.1",
		"@codemirror/view": "^6.26.3"
	}
}
